{
	"name": "@ribarich/woocommerce-nft-downloads",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build:js": "wp-scripts build",
		"build:scss": "sass scss:build/css",
		"watch:js": "wp-scripts start",
		"watch:scss": "sass scss:build/css --watch",
		"test:php": "wp-env run phpunit 'phpunit -c /var/www/html/wp-content/plugins/woocommerce-nft-downloads/phpunit.xml --verbose'",
		"test:php:api": "wp-env run phpunit 'phpunit -c /var/www/html/wp-content/plugins/woocommerce-nft-downloads/phpunit.xml --verbose --group remote'",
		"test:e2e": "wp-scripts test-e2e",
		"test": "npm run test:php; npm run test:php:api; npm run test:e2e",
		"test:js": "wp-scripts test-unit-js",
		"lint:php": "./vendor/bin/phpcs -s .",
		"lint:php:fix": "./vendor/bin/phpcbf .",
		"lint:js": "wp-scripts lint-js",
		"lint:js:fix": "wp-scripts format '*.js' .wp-env.json package.json composer.json scripts/ js/",
		"lint:scss:fix": "wp-scripts lint-style scss/ --fix",
		"format": "concurrently \"npm run lint:js:fix\" \"npm run lint:php:fix\" \"npm run lint:scss:fix\"",
		"start": "wp-env start --xdebug"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
