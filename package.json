		"@wordpress/e2e-test-utils": "5.4.10",
		"@wordpress/scripts": "19.2.2",
		"adm-zip": "0.5.9",
		"commander": "8.3.0",
		"concurrently": "6.4.0",
		"glob-all": "3.2.1",
		"prettier": "npm:wp-prettier@2.2.1-beta-1",
		"sass": "1.45.0"
	},
	"dependencies": {
		"@emotion/react": "11.7.1",
		"@emotion/styled": "11.6.0",
		"@mui/icons-material": "5.2.5",
		"@mui/lab": "5.0.0-alpha.64",
